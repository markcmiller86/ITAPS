	<style type="text/css">

	
	* {
		font-family:arial,tahoma,verdana,helvetica;
		font-size:12px;
	}

	/* the menu */

	ul,li,a {
		display:block;
		margin:0;
		padding:0;
		border:0;
                color:white;
	}

	ul {
		width:150px;
		border:1px solid #9d9da1;
		/*background:white;*/
		list-style:none;
                background-color: #4682b4;
                color:black;

	}

	li {
		position:relative;
		padding:1px;
		padding-left:1px;
		z-index:9;
                background-color: #4682b4;
                color:black;
	}
		li.folder ul {
			position:absolute;
			left:120px; /* IE */
			top:5px;
		}		
			li.folder>ul { left:140px; } /* others */

	a {
		padding:2px;
		border:1px solid white;
		text-decoration:none;
		color:black;
		font-weight:bold;
		width:100%; /* IE */
	}
		li>a { width:auto; } /* others */

	li a.submenu {
		background:url("./sub.gif") right no-repeat;
	}

	/* regular hovers */

	a:hover {
		border-color:gray;
		background-color:#bbb7c7;
		color:black;
	}
		li.folder a:hover {
			background-color:#bbb7c7;
		}
	
	/* hovers with specificity */
	
	li.folder:hover { z-index:20; }		
		
	ul ul, li:hover ul ul {
		display:none;
	}

	li:hover ul, li:hover li:hover ul {
		display:block;
	}		

	</style>
	

